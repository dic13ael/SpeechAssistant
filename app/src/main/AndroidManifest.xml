<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tnx097.speechassistant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RestaurantActivity"
            android:label="Restaurang"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MessageActivity"
            android:label="Meddelande"
            android:parentActivityName=".RestaurantActivity" />
        <activity
            android:name=".ListenActivity"
            android:label="Lyssnar"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".OvrigtActivity"
            android:label="Övrigt"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".PersonActivity"
            android:label="Hälsningsfras"
            android:parentActivityName=".MainActivity"
            >

        </activity>

    </application>

</manifest>